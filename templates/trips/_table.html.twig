<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Vehicle</th>
        <th scope="col">Driver</th>
        <th scope="col">Date</th>
    </tr>
    </thead>
    <tbody>

    {% for trip in trips %}
        <tr>
            <th scope="row">{{ trip.id() }}</th>
            <td>{{ trip.vehicle().description() }}</td>
            <td>{{ trip.driver().description() }}</td>
            <td>{{ trip.date()|date('d/m/Y') }}</td>
        </tr>
    {% endfor %}

    </tbody>
</table>