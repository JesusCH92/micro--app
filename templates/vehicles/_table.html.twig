<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Brand</th>
        <th scope="col">Model</th>
        <th scope="col">Plate</th>
        <th scope="col">License Required</th>
        <th scope="col">Actions</th>
    </tr>
    </thead>
    <tbody>

    {% for vehicle in vehicles %}
        <tr>
            <th scope="row">{{ vehicle.id() }}</th>
            <td>{{ vehicle.brand().value() }}</td>
            <td>{{ vehicle.model().value() }}</td>
            <td>{{ vehicle.plate().value() }}</td>
            <td>{{ vehicle.licenseRequired().value() }}</td>
            <td>
                <a href="{{ path('app_vehicle_deleter', {'id': vehicle.id()}) }}">Delete</a>
            </td>
        </tr>
    {% endfor %}

    </tbody>
</table>
<a href="{{ path('app_vehicle_creator') }}">Create</a>
